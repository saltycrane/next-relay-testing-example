    "next": "^12.1.4",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-relay": "^13.2.0",
    "relay-runtime": "^13.2.0",
    "typescript": "^4.6.3"
  },
  "devDependencies": {
